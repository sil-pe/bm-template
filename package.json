{
  "name": "bm-template",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "engines": {
    "node": "10.7.*",
    "npm": "6.1.*"
  },
  "author": "bettermarks GmbH <bmdevops@bettermarks.com>",
  "private": true,
  "scripts": {
    "all-checks": "npm run tsc; npm run test -- --reporter dot; npm run tslint; npm run stylelint",
    "preinstall": "npx check-node-version --package",
    "prebuild": "TARGET=SERIESPLAYER NODE_ENV=production npm run build:css ",
    "build": "TARGET=SERIESPLAYER NODE_ENV=production webpack --config webpack/seriesplayer.config.js --mode production --profile --json > webpack/stats/build.json",
    "build:css": "mkdir -p webpack/stats && webpack --config webpack/css.config.js --profile --json > webpack/stats/build-css.json",
    "build:css:js": "mkdir -p webpack/stats && webpack --config webpack/css.config.js --config webpack/js.config.js --profile --json > webpack/stats/build-js-css.json",
    "clear-cache": "rm -rf .cache/*",
    "check": "npx npm-check -u --skip-unused",
    "doctoc": "doctoc",
    "test": "TS_NODE_CACHE_DIRECTORY=.cache/ts-node CACHE_REQUIRE_PATHS_FILE=.cache/require.json mocha --opts mocha.opts",
    "test:coverage": "nyc npm test",
    "test:ci": "nyc npm test -- --reporter mocha-junit-reporter",
    "test:debug": "TS_NODE_CACHE_DIRECTORY=.cache/ts-node CACHE_REQUIRE_PATHS_FILE=.cache/require.json mocha $NODE_DEBUG_OPTION  --opts mocha.opts",
    "test:auto": "nodemon --exec npm test -- --inspect",
    "test:webdriver": "ts-node test/webdriver/test.ts",
    "prestart": "NODE_ENV=development TARGET=SERIESPLAYER npm run build:css",
    "start": "NODE_ENV=development TARGET=SERIESPLAYER npm run start:only",
    "start:only": "NODE_ENV=development TARGET=SERIESPLAYER webpack-dev-server --config webpack/seriesplayer.config.js --mode development",
    "tsc": "tsc --noEmit",
    "tsc:dist": "tsc --outDir dist/tsc --traceResolution true --diagnostics true",
    "tslint": "tslint -c tslint.json --project tsconfig.json -t stylish",
    "tslint:fix": "npm run tslint -- --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:test": "stylelint test/linter.specss --syntax scss > test/linter.specss.out || git diff --exit-code test/linter.specss.out",
    "doc": "typedoc --out doc",
    "styleguide": "NODE_ENV=development npm run build:css:js && styleguidist server --config styleguide.config.js",
    "styleguide:build": "NODE_ENV=production npm run build:css:js && styleguidist build --config styleguide.config.js",
    "styleguide:test": "echo \"Warning: no test specified\" && exit 0",
    "http-dist": "ts-node lib/http-dist"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "src/**/*.spec.ts",
      "src/**/*.spec.tsx"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true,
    "cache": true
  },
