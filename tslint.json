{
    "extends": [
      "bm-tslint-rules"
    ],
    "rulesDirectory": "node_modules/tslint-no-unused-expression-chai/rules",
    "rules": {
      "no-unused-expression": false,
      "no-unused-expression-chai": [
        true,
        "allow-fast-null-checks"
      ],
      "no-single-line-block-comment": false
    }
}
